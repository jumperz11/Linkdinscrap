# Environment Variables
# Copy this file to .env and fill in your values

# OpenAI API Key (for AI scoring and message generation)
OPENAI_API_KEY=your_openai_api_key_here

# Server Port
PORT=3000

# Session Settings
MAX_PROFILES_PER_SESSION=100
MAX_SESSION_DURATION_MINUTES=60
MIN_DELAY_MS=3000
MAX_DELAY_MS=8000

# Scoring Threshold (0-100)
CONNECT_THRESHOLD=75
